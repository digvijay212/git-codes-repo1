 public static void main(String[] args) {
         Scanner scn=new Scanner(System.in);
        
        int n=scn.nextInt();
        
        int arr[]=new int[n];
       
        for(int i=0;i<n;i++){
            arr[i]=scn.nextInt();

        }
        Arrays.sort(arr);
        for(int i=n-2;i>=0;i--){
        if(arr[i]!=arr[n-1]){
            System.out.print(arr[i]);
            return;
        }
        }
            System.out.println(-2147483648);
        }
        }
        

 public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        String str =sc.nextLine();
        int n= str.length();
        StringBuffer sb =new StringBuffer(str);
        System.out.println(sb);
        sb.append(str);
        for(int i=n-1;i>=1;i--){
            for (int j=0;j<n;j++){
                System.out.print(sb.charAt(i+j));
            }
            System.out.println();
            
        }
        
        
    }
}
